# URL node WebSocket (Alchemy, Infura, dll.)
ALCHEMY_WSS_URL=wss://<node_url>

# Private key wallet Anda
PRIVATE_KEY=<private_key_wallet_anda>

# Alamat wallet tujuan transfer token
VAULT_WALLET_ADDRESS=<address_wallet_vault>

# Alamat kontrak token
TOKEN_ADDRESS=<address_kontrak_token>

# URL halaman klaim token
CLAIM_URL=<url_halaman_klaim>

# Selector tombol login di halaman klaim
LOGIN_BUTTON_SELECTOR=<selector_login>

# Selector tombol klaim di halaman klaim
CLAIM_BUTTON_SELECTOR=<selector_klaim>

# Interval waktu klaim (dalam detik)
CLAIM_INTERVAL=30

# Jumlah token yang akan ditransfer
TRANSFER_AMOUNT=10
